qt_add_qml_module(ThermostatApp
    URI "Main"
    VERSION 1.0
    QML_FILES
        Main.qml
)

add_subdirectory(imports)
add_subdirectory(content)

target_link_libraries(ThermostatApp PRIVATE
    ThermostatContentplugin
    ThermostatCustomControlsplugin
    Thermostatplugin
)
